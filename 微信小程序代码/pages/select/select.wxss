.allbody{
  height: 100%;
  width: 100%;
  position: relative;
}
.selectview{
  margin-top: 10px;
  height: 60px;
  width: 100%;
}
.selectimg{
  height: 30px;
  border: 1px solid #a6a6a6;
  position: absolute;
  width: 10%;
  margin-left: 10%;
  margin-top: 5px;
  border-radius: 10px 0px 0px 10px;
  overflow: hidden;
}
.selectview input{
  position: absolute;
  border: 1px solid #a6a6a6;
  height: 30px;
  width: 50%;
  margin-left: 20%;
  margin-top: 5px;
}
image{
  height: 100%;
  width: 100%;
}
.selectbutton{
  height: 32px;
  width: 20%;
  text-align: center;
  line-height: 30px;
  font-size: 10px;
  border: 1px solid #a6a6a6;
  position: absolute;
  margin-left: 70%;
  margin-top: 5px;
  border-radius: 0px 10px 10px 0px;
}
.selectbody{ 
  width: 80%;
  height: auto;
  margin-top: 3%;
  margin-left: 10%;
  background-color: #e6ebea;
  -webkit-box-shadow:0px 0px 10px #a6a6a6; 
}
.celectplace{
  height: auto;
  width: 90%;
  box-sizing: border-box;
  padding: 10px;
  margin: 5%;
  border-radius: 10px;
  background-color: rgba(156, 156, 156, 0.911);
}
.celectplace view{
  width: 100%;
  word-wrap: break-word
}
.toptop{
  height: 5px;
  width: 100%;
}
.selcetxx{
  font-size: 12px;
  text-indent: 2em;
}
.selcetwhere{
  font-size: 20px;
  color: rgba(27, 61, 212, 0.808);
  font-weight: bolder;
}