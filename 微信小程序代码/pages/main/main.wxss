}

.alldown {
  height: 110px;
  width: 100%;
  text-align: center;
  line-height: 130px;
  animation: ziti 2s infinite alternate;
}

@keyframes ziti {
  0% {
    font-size: 20px;
    color: green;
  }

  100% {
    font-size: 14px;
    color: red;
  }
}

