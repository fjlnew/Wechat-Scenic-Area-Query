}

.blackbox {
  height: 30px;
  width: 30px;
  position: absolute;
  margin-left: 90%;
  margin-top: -25px;
  background-color: black;
  animation: blackshow 3s infinite alternate linear;
}

@keyframes blackshow {
  0% {
    transform: rotate(0deg);
    margin-left: 90%;
    background-color: green;
  }

  100% {
    transform: rotate(360deg);
    margin-left: 70%;
    background-color: red;
  }
}

.city {
  height: 30px;
