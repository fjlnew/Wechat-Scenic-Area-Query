.loginbox{
  height: 60vh;
  width: 60%;
  margin-left: 20%;
  margin-top: 20%;
  -webkit-box-shadow: 0px 0px 10px #A6A6A6;
  font-size: 12px;
  border-radius: 10px;
  position: relative;
}
.login-top{
  height: 45px;
  width: 100%;
}
.login-button{
  height: calc(100% - 45px );
  width: 100%;
}
.loginview1{
  height: 100%;
  width: 50%;
  text-align: center;
  line-height: 40px;
}
.loginview2{
  height:calc(100% - 2px);
  width: 50%;
  text-align: center;
  border-bottom: 1px solid #A6A6A6;
  line-height: 40px;
}
.zhuceview1{
  height: 100%;
  width: 50%;
  margin-left: 50%;
  margin-top: -40px;
  text-align: center;
  line-height: 40px;
}
.zhuceview2{
  height: calc(100% - 2px);
  width: 50%;
  margin-left: 50%;
  margin-top: -40px;
  text-align: center;
  border-bottom: 1px solid #A6A6A6;
  line-height: 40px;
}
.login-button .login{
  height: 100%;
  width: 100%;
}
.login-button .login .ziti{
  height: 20%;
  text-align: center;
  font-size: 15px;
  line-height: 70px;
}
.login-button .login input{
  height: 30px;
  width: calc(100% - 30px);
  margin-left: 30px;
  margin-top: -1px;
  border: 1px solid #A6A6A6;
  border-radius: 10px;
  text-align: center;
  font-size: 10px;
}
.zhanghao{
  height: 30px;
  width: 80%;
  margin-top: 5px;
  margin-left: 10%;
  border: 1px solid #A6A6A6;
  border-radius: 10px;
  text-align: center;
}
.mima{
  height: 30px;
  width: 80%;
  margin-top: 5px;
  margin-left: 10%;
  border: 1px solid #A6A6A6;
  border-radius: 10px;
  text-align: center;
  position: relative;
}
.login-button .login .mimakuang{
  width: calc(100% - 60px);
  border-radius: 0px 0px 0px 0px;
}
.login-button .login .name{
  border-radius: 0px 10px 10px 0px;
}
.login-button .login button{
  height: 30px;
  width: 80%;
  margin-top: 20px;
  background-color: white;
  color: black;
  font-size: 12px;
  border-radius: 10px;
  border: 1px solid #A6A6A6;
}
.wihte{
  height: 5%;
  width: 100%;
  background-color: transparent;
}
.showpassworld{
  height: 30px;
  width: 30px;
  margin-top: -32px;
  margin-left: calc(100% - 30px);
  position: absolute;
  border: 1px solid #A6A6A6;
  border-radius: 0px 10px 10px 0px;
}
image{
  height: 100%;
  width: 100%;
}
.losemima{
  height: 10px;
  width: 50px;
  font-size: 10px;
  position: absolute;
  margin-left:calc(50% - 25px);
  margin-top: 10px;
}
.tupiankuang{
  height: 30px;
  width: 30px;
  position: absolute;
  float: left;
  overflow: hidden;
}
.zhuce .wihte{
  margin-top: 10px;
  height: 40px;
  width: 100%;
  text-align: center;
  font-size: 15px;
  line-height: 40px;

}
.zhuce input{
  height: 20px;
  width: 80%;
  text-align: center;
  margin-top: 18px;
  border-radius: 10px;
  border: 1px solid #A6A6A6;
  margin-left: 10%;
}
.zhuce button{
  height: 30px;
  width: 80%;
  font-size: 10px;
  border: 1px solid #A6A6A6;
  margin-top: 20px;
  border-radius: 10px;
}